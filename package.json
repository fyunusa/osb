{
  "name": "osbts",
  "version": "1.0.3",
  "description": "A TypeScript-based code generator for models, DTOs, entities, services, and controllers",
  "main": "dist/packages/core/index.js",
  "types": "dist/packages/core/index.d.ts",
  "bin": {
    "osb": "dist/packages/cli/index.js"
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "osb-generate": "node dist/packages/cli/index.js",
    "prepare": "pnpm build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "generator",
    "nestjs",
    "dto",
    "entity",
    "cli"
  ],
  "author": "Umar Dev",
  "license": "ISC",
  "packageManager": "pnpm@10.7.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^14.0.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@types/node": "^24.3.0"
  }
}
